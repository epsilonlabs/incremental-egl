<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="org.eclipse.epsilon.egl.incremental.dt.launching.EglLaunchConfigurationDelegate"
            id="org.epsilon.egl.eclipse.incremental.dt.launching.EglLaunchConfigurationDelegate"
            modes="run,debug"
            name="Incremental EGL Template">
         xxx
      </launchConfigurationType>
   </extension> 
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="org.eclipse.epsilon.egl.dt.launching.EglLaunchConfigurationTabGroup"
            description="Create a configuration to launch an EGLtemplate."
            id="org.eclipse.epsilon.egl.incremental.dt.launching.EglLaunchConfigurationTabGroup"
            type="org.epsilon.egl.eclipse.incremental.dt.launching.EglLaunchConfigurationDelegate"/>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="org.epsilon.egl.eclipse.incremental.dt.launching.EglLaunchConfigurationDelegate"
            icon="icons/egl.png"
            id="EglProgramInc"/>
   </extension>
</plugin>
